<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             android:background="@color/white" android:id="@+id/constraintLayout8">

    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="32dp"
            android:padding="12dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
        <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:orientation="vertical" app:layout_constraintTop_toTopOf="parent"
                                              app:layout_constraintBottom_toBottomOf="parent"
                                              app:layout_constraintEnd_toEndOf="parent"
                                              app:layout_constraintStart_toStartOf="parent"
                                              app:layout_constraintGuide_percent="0.6" android:id="@+id/guideline22"/>
        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/lab_confirmfacturedialog_totalHT"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="Total  HT : "
                android:textColor="@android:color/background_dark" android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline22"
                android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                android:layout_marginBottom="32dp"
                app:layout_constraintBottom_toTopOf="@+id/lab_confirmfacturedialog_totaltva"
                android:layout_marginTop="32dp" app:layout_constraintTop_toTopOf="parent"/>

        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/lab_confirmfacturedialog_totaltva"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="Total TVA : "

                android:textColor="@android:color/background_dark"
                app:layout_constraintEnd_toStartOf="@+id/guideline22"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/txt_confirmfacturedialog_totaltva"/>

        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/lab_confirmfacturedialog_totalTimbre"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="Total Timbre : "
                android:textColor="@android:color/background_dark"
                android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline22"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/txt_confirmfacturedialog_totalTimbre"/>

        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/lab_confirmfacturedialog_totalTTC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="Totale TTC : "
                android:textColor="@android:color/background_dark"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline22" android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/txt_confirmfacturedialog_totalTTC"/>

        <!-- hhh-->
        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/txt_confirmfacturedialog_totalHT"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="@string/default_textvalue"
                android:textColor="@android:color/background_dark"
                android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/guideline22"
                android:layout_marginTop="32dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginBottom="16dp"
                app:layout_constraintBottom_toTopOf="@+id/txt_confirmfacturedialog_totaltva"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>

        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/txt_confirmfacturedialog_totaltva"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="@string/default_textvalue"

                android:textColor="@android:color/background_dark"
                app:layout_constraintStart_toStartOf="@+id/guideline22" android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/txt_confirmfacturedialog_totalHT"
                android:layout_marginBottom="11dp"
                app:layout_constraintBottom_toTopOf="@+id/txt_confirmfacturedialog_totalTimbre"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"/>

        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/txt_confirmfacturedialog_totalTimbre"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="@string/default_textvalue"
                android:textColor="@android:color/background_dark"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/txt_confirmfacturedialog_totaltva"
                app:layout_constraintStart_toStartOf="@+id/guideline22" android:layout_marginBottom="12dp"
                app:layout_constraintBottom_toTopOf="@+id/txt_confirmfacturedialog_totalTTC"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"/>

        <android.support.v7.widget.AppCompatTextView
                android:id="@+id/txt_confirmfacturedialog_totalTTC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="0.25"
                android:text="@string/default_textvalue"
                android:textColor="@android:color/background_dark"
                app:layout_constraintStart_toStartOf="@+id/guideline22"
                android:layout_marginStart="8dp" android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/txt_confirmfacturedialog_totalTimbre"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"/>
    </android.support.constraint.ConstraintLayout>

    <android.support.v7.widget.AppCompatButton
            android:id="@+id/btn_confirmfacturedialog_passerfacture"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:background="@color/colorPrimary"
            android:text="PASSER LA FACTURE"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout"
            app:layout_constraintTop_toTopOf="@+id/guideline5"/>

    <android.support.v7.widget.AppCompatButton
            android:id="@+id/btn_confirmfacturedialog_annulerfacture"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@drawable/shap_btn_annuler"
            android:text="ANNULER LA FACTURE"
            app:layout_constraintEnd_toEndOf="@+id/btn_confirmfacturedialog_passerfacture"
            app:layout_constraintStart_toStartOf="@+id/btn_confirmfacturedialog_passerfacture"
            app:layout_constraintTop_toBottomOf="@+id/btn_confirmfacturedialog_passerfacture"/>

    <android.support.constraint.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6"/>
</android.support.constraint.ConstraintLayout>